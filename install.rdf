        <em:targetApplication>
            <Description>
                <!-- Komodo Edit's uuid -->
                <em:id>{b1042fb5-9e9c-11db-b107-000d935d3368}</em:id>
                <em:minVersion>8.5</em:minVersion>
                <em:maxVersion>8.*</em:maxVersion>
            </Description>
        </em:targetApplication>
    </Description>
