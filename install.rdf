     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
    <Description about="urn:mozilla:install-manifest">
        <em:unpack>true</em:unpack>
        <em:id>skin_classic@activestate.com</em:id>
        <em:name>Classic Skin</em:name>
        <em:version>1.0</em:version>
        <em:description>This skin will remove any custom styling that was added in Komodo 8 and rely only on your system to do the styling.</em:description>
        <em:creator>Nathan Rijksen</em:creator>
        <em:homepageURL>http://komodo.activestate.com/addons/skin_classic/</em:homepageURL>
        <em:iconURL>chrome://skin-classic/skin/icon.png</em:iconURL>
        <em:type>2</em:type> <!-- type=extension --> 

        <em:targetApplication>
            <Description>
                <!-- Komodo IDE's uuid -->
                <em:id>{36E66FA0-F259-11D9-850E-000D935D3368}</em:id>
                <em:minVersion>8.5</em:minVersion>
                <em:maxVersion>8.*</em:maxVersion>
